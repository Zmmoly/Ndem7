<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">
    <!-- Base gradient -->
    <item>
        <shape>
            <gradient
                android:angle="135"
                android:startColor="#F5F1E8"
                android:centerColor="#EDE8DC"
                android:endColor="#E5DFD0"
                android:type="linear" />
        </shape>
    </item>
    
    <!-- Overlay pattern effect -->
    <item>
        <shape>
            <solid android:color="#08000000" />
        </shape>
    </item>
</layer-list>
